{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/drivebase/src",
  "defaultLibraryPrefix": "@drivebase",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/drivebase/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "cron": {
      "type": "application",
      "root": "apps/cron",
      "entryFile": "main",
      "sourceRoot": "apps/cron/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "drivebase": {
      "type": "application",
      "root": "apps/drivebase",
      "entryFile": "main",
      "sourceRoot": "apps/drivebase/src",
      "compilerOptions": {
        "tsConfigPath": "apps/drivebase/tsconfig.app.json",
        "typeCheck": false,
        "deleteOutDir": true
      }
    },
    "files": {
      "type": "library",
      "root": "libs/files",
      "entryFile": "index",
      "sourceRoot": "libs/files/src",
      "compilerOptions": {
        "tsConfigPath": "libs/files/tsconfig.lib.json"
      }
    },
    "providers": {
      "type": "library",
      "root": "libs/providers",
      "entryFile": "index",
      "sourceRoot": "libs/providers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/providers/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "workspaces": {
      "type": "library",
      "root": "libs/workspaces",
      "entryFile": "index",
      "sourceRoot": "libs/workspaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/workspaces/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/drivebase"
}