{
  "name": "@drivebase/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for Drivebase",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "bun test"
  },
  "keywords": ["drivebase", "sdk", "storage", "cloud", "typescript"],
  "author": "Drivebase Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@drivebase/core": "workspace:*"
  }
}
